      <field name="view_mode">tree,form</field>
    </record>


    <menuitem name="Empresas Contratista" id="empresa_contratista.menu_root" />

    <menuitem name="Empresas" id="empresa_contratista.menu_1"
      parent="empresa_contratista.menu_root" action="empresa_contratista_action_window" /> -->

  </data>
</odoo>